{
  "name": "location-backend",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "turbo dev --filter=web",
    "build": "turbo build --filter=web",
    "lint": "turbo lint --filter=web",
    "test": "turbo test --filter=web",
    "format": "turbo format --filter=web",
    "db:generate": "prisma generate --schema=db/schema.prisma",
    "db:push": "prisma db push --schema=db/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=db/schema.prisma",
    "db:studio": "prisma studio --schema=db/schema.prisma",
    "db:pull": "prisma db pull --schema=db/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "dotenv": "^16.6.1"
  },
  "devDependencies": {
    "turbo": "^2.1.1",
    "prisma": "^5.20.0"
  }
}
