{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
      "POSTGRES_URL",
      "AUTH0_DOMAIN",
      "AUTH0_CLIENT_ID",
      "AUTH0_CLIENT_SECRET",
      "APP_BASE_URL",
      "AUTH0_SECRET",
      "AUTH_SECRET",
      "BACKEND_API_URL"
    ],
  "tasks": {
    "build": {
      "outputs": [".next/**"]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  }
}
